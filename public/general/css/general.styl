body
	background-color #f5f6fa
	font-size 14px
	font-family 'Roboto', sans-serif
	
h2,h3,h4,h5,ul,body,html
	padding 0px
	margin 0px
	
.mtop16
	margin-top 16px

.mbottom16
	margin-bottom 16px
	
.wrapper
	min-height 100vh
	overflow hidden
	.col1
		float left
		position relative
		z-indez 999
		width 17%
	.col2
		float left
		width 83%
		
.sidebar
	background-color #fff
	min-height 100vh
	.section-top
		background-color #00acff
		box-shadow 0 0.5rem 1rem rgba(0,0,0,0.15)
	.logo
		display block
		margin 0px auto
		padding 15px 0px
		width 60%
	.main
		display block
		margin-top 8px
		ul
			li
				margin-bottom 4px
				a
					color #8998AC !important
					display block
					font-size 1.1em 
					padding 12px 12px 
					i 
						margin-right 10px
					& hover
						box-shadow 0 0.5rem 1rem rgba(0,0,0,0.15)
						background-color #00acff
						color #fff !important
						text-decoration none
				a.active
					box-shadow 0 0.5rem 1rem rgba(0,0,0,0.15)
					background-color #00acff
					color #fff !important
					text-decoration none
.navbar
	background-color #00acff
	.navbar-nav
		a
			color #fff !important

	
.translate
	color #000 !important
	
.page
	padding-bottom 16px
	.breadcrumb
		background-color #fff
		box-shadow 0 0.5rem 1rem rgba(0,0,0,0.10) !important
		a
			& hover
				text-decoration none
	.panel
		background-color #fff
		border-radius 6px
		border-bottom 2px solid #00acff
		width 100%
		.header
			border-bottom 1px solid #e8e8e8
			h2.title
				display inline-block
				color #19213D
				font-size 1.1em
				padding 12px 16px
		.inside
			padding 16px
			.nav
				.nav-link
					border 1px solid #00acff
					margin-right 8px
					& hover
						background-color #00acff
						color #fff					

.lateral
	margin 0px
	padding 0px
	background-color #fff
	min-height 100vh
	box-shadow 0 0.5rem 1rem rgba(0,0,0,0.15)
	.acordion
		padding 0px !important
		.card
			margin 0px !important
			padding 0px !important

.card-header
	padding 0.2em 0 0.2em 0 !important
	color #fff
	
.btn
	font-size 1.3em
		
#shell
	margin 0px auto
	padding 80px

.puzzle 
	width 10em
	height 10em
	line-height 100px
	text-align center
	background-size cover
	position relative
	float left
	margin 0px
	cursor pointer
	.t 
		width 2.7em
		height 2.7em
		position absolute
		top -2.3em
		left 3.65em
		-webkit-border-radius 100%
		-moz-border-radius 100%
		border-radius 100%
	.r 
		width 2.7em
		height 2.7em
		position absolute
		top 3.65em
		left 9.6em
		-webkit-border-radius 100%
		-moz-border-radius 100%
		border-radius 100%

.text 
	display block
	color #fff
	font-size 3em
	text-align center
	padding 0.2em 0 0 0.4em
	position relative
	background transparent !important

